<!-- model IEnumerable<BookLibraryEF.Model.Book> -->



@foreach (var item in Model)
{

    <div style="margin-top:40px">

        <div class="well col-md-6">
            <div class="row">
                <div class="pull-right">
                    <img height="100" width="80" src="@item.ImageUrl" />

                </div>

                <dl class="dl-horizontal">
                    <dt>
                        @item.ID

                    </dt>
                    <dt>
                        @item.Title

                    </dt>
                    <dt>
                        @item.Price

                    </dt>
                    <dt>
                        @item.Discription

                    </dt>
                    <dt>
                        @item.PublishDate

                    </dt>
                    <dt>
                        @item.CategoryId

                    </dt>

                </dl>

            </div>

        </div>


    </div> }

